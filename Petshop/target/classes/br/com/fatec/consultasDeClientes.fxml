<AnchorPane id="AnchorPane" prefHeight="850.0" prefWidth="850.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="br.com.fatec.Controller.ConsultasDeClientesController">
    <stylesheets>
        <URL value="@/br/com/fatec/css/consultasdeclientes.css" />
    </stylesheets>
   <children>
      <Label layoutX="102.0" layoutY="206.0" stylesheets="@css/default.css" text="Pesquisar por:">
         <font>
            <Font name="Yu Gothic UI Semibold" size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="cmTipoPesquisa" layoutX="91.0" layoutY="230.0" onAction="#cmTipoPesquisa_selected" prefHeight="43.0" prefWidth="162.0" stylesheets="@css/default.css" />
      <Label layoutX="300.0" layoutY="206.0" stylesheets="@css/default.css" text="Dado:">
         <font>
            <Font name="Yu Gothic UI Semibold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtDados" layoutX="275.0" layoutY="230.0" onKeyReleased="#txtDados_KeyReleased" prefHeight="43.0" prefWidth="229.0" styleClass="input" stylesheets="@css/default.css">
         <font>
            <Font name="Yu Gothic UI Semibold" size="14.0" />
         </font>
      </TextField>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#052f5f" height="146.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="904.0" />
      <Button layoutX="41.0" layoutY="51.0" mnemonicParsing="false" onAction="#switchPage_home" styleClass="btnVoltar" stylesheets="@css/default.css" text="❮  voltar" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <TableView fx:id="tbvPet" layoutX="84.0" layoutY="299.0" prefHeight="511.0" prefWidth="424.0" stylesheets="@css/default.css">
         <columns>
            <TableColumn fx:id="nome" prefWidth="239.0" text="Nome" />
            <TableColumn fx:id="genero" prefWidth="101.0" text="Gênero" />
         </columns>
      </TableView>
      <Button fx:id="btnDeletar" layoutX="653.0" layoutY="550.0" mnemonicParsing="false" onAction="#btnDeletar_click" styleClass="btnDeletar" stylesheets="@css/default.css" text="Deletar ✘" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnEditar" layoutX="669.0" layoutY="433.0" mnemonicParsing="false" onAction="#btnEditar_click" styleClass="btnEditar" stylesheets="@css/default.css" text="Editar ☛" textFill="#df3b57">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnPesquisar" layoutX="651.0" layoutY="251.0" mnemonicParsing="false" onAction="#btnPesquisar_click" prefHeight="44.0" prefWidth="142.0" styleClass="btnPesquisar" stylesheets="@css/default.css" text="🔍 Pesquisar" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
